GOOGLE_VISION_API_URL=https://vision.googleapis.com/v1/images:annotate
GOOGLE_TIMEOUT=30

# Fonctionnalités IA
AI_RENT_PREDICTION_ENABLED=true
AI_RISK_ASSESSMENT_ENABLED=true
AI_CONTRACT_GENERATION_ENABLED=true
AI_IMAGE_ANALYSIS_ENABLED=true
AI_RECOMMENDATIONS_ENABLED=true
AI_CUSTOMER_RESPONSE_ENABLED=true
AI_MARKET_ANALYSIS_ENABLED=true

